{
  "name": "narloader",
  "version": "1.0.0",
  "description": "NarLoader - Nanika ARchive Loader",
  "keywords": [
    "nar",
    "ukagaka"
  ],
  "main": "./dist/lib/NarLoader.js",
  "types": "./dist/lib/NarLoader.d.ts",
  "files": [
    "dist",
    "web"
  ],
  "scripts": {
    "dist": "npm run node && npm run web",
    "node": "rm -rf dist && cross-env NODE_ENV=production webpack",
    "web": "rm -rf web && cross-env NODE_ENV=production webpack --config ./webpack.config.web.js",
    "lint": "npm run eslint && npm run tslint",
    "eslint": "eslint *.js src test",
    "tslint": "tslint  tslint *.ts src/**/*.ts test/**/*.ts",
    "test": "npm run test-node && npm run test-web",
    "test-node": "cross-env NODE_ENV=test nyc mocha",
    "test-web": "cross-env NODE_ENV=test karma start --single-run",
    "docdist": "cd doc && git clean -fd && git rm -rf * && cd .. && npm run doc && cd doc && touch .nojekyll && git add . && git commit -m doc && git push",
    "doc": "typedoc --mode modules --out doc --readme Readme.md --name \"API Document\" --disableOutputCheck --includeDeclarations --excludeNotExported src && cd doc && git status && cd .."
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "fso": "^2.5.7",
    "jszip": "^3.1.3",
    "nanika-storage": "^2.6.0"
  },
  "devDependencies": {
    "@types/encoding-japanese": "^1.0.15",
    "@types/jszip": "3.1.3",
    "@types/mocha": "^2.2.33",
    "@types/node": "^8.5.7",
    "@types/power-assert": "^1.4.29",
    "babel-core": "^6.18.2",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.18.0",
    "browserfs": "^1.1.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.14.0",
    "eslint-config-narazaka": "^1.0.1",
    "eslint-loader": "^1.6.1",
    "espower-ts-node": "^1.0.3",
    "istanbul-instrumenter-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-config-narazaka-ts-js": "^1.0.2",
    "karma-coverage": "^1.1.1",
    "karma-detect-browsers": "^2.2.3",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-own-reporter": "^1.1.2",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^2.0.9",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "phantomjs-prebuilt": "^2.1.13",
    "power-assert": "^1.4.2",
    "ts-loader": "^3.2.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.2.0",
    "typedoc": "^0.9.0",
    "typescript": "2.6.2",
    "webpack": "^3.10.0",
    "webpack-config-narazaka-ts-js": "1.0.0"
  },
  "author": {
    "name": "narazaka",
    "url": "https://narazaka.net/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ikagaka/NarLoader.git"
  }
}
